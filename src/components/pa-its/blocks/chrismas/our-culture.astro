            <h3 class="text-xl font-bold">{feature.title}</h3>
          </div>
          <ul class="space-y-3 text-sm text-gray-100 leading-relaxed">
            {feature.items.map((item) => (
              <li class="flex items-start gap-3">
                <span class="text-white">☑️</span>
                <span>{item.text}</span>
              </li>
            ))}
          </ul>
        </div>
      ))
    }
  </div>
</section>
